    "quote_group": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SYMBOL",
          "name": "_quoted_line"
        }
      }
    },
    "quoted_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "quote_marker"
